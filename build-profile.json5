  "modules": [
    {
      "name": "entry",
      "srcPath": "./entry",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "home",
      "srcPath": "./feature/home",
    },
    {
      "name": "ai",
      "srcPath": "./feature/ai",
    },
    {
      "name": "rag",
      "srcPath": "./feature/rag",
    },
    {
      "name": "mcp",
      "srcPath": "./feature/mcp",
    },
    {
      "name": "profile",
      "srcPath": "./feature/profile",
    }
  ]
